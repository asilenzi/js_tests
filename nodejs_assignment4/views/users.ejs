  <main>
    <% if (users.length > 0) { %>
      <ul>
        <% for (let user of users) { %>
          <li><%= user.username %></li>
        <% } %>
      </ul>
    <% } else { %>
      <h1>No Users Found!</h1>
    <% } %>
  </main>
